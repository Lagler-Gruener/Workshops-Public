{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    },
    "functions": [],
    "variables": {
        "protectionContainer": "[concat('iaasvmcontainer;iaasvmcontainerv2;', resourceGroup().name, ';', 'acpdemolevel1')]",
        "protectedItem": "[concat('vm;iaasvmcontainerv2;', resourceGroup().name, ';', 'acpdemolevel1')]",
        "backupFabric": "Azure"
    },
    "resources": [
         {
            "name": "straccacpdemolevel1",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2019-06-01",
            "location": "[resourceGroup().location]",
            "tags": {

            },
            "sku": {
                "name": "Standard_LRS"
            },
            "kind": "Storage"
        },        
        {
            "name": "acpdemolevel1-PublicIP",
            "type": "Microsoft.Network/publicIPAddresses",
            "apiVersion": "2019-11-01",
            "location": "[resourceGroup().location]",
            "tags": {                
            },
            "properties": {
                "publicIPAllocationMethod": "Dynamic",
                "dnsSettings": {
                    "domainNameLabel": "[toLower('acpdemolevel1')]"
                }
            }
        },
        {
            "name": "acpdemolevel1-nsg",
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2018-08-01",
            "location": "[resourceGroup().location]",
            "properties": {
                "securityRules": [
                    {
                        "name": "nsgRule1",
                        "properties": {
                            "description": "description",
                            "protocol": "Tcp",
                            "sourcePortRange": "*",
                            "destinationPortRange": "3389",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 100,
                            "direction": "Inbound"
                        }
                    }
                ]
            }
        },
        {
            "name": "acpdemolevel1-VirtualNetwork",
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2019-11-01",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/networkSecurityGroups', 'acpdemolevel1-nsg')]"
            ],
            "tags": {
                "displayName": "acpdemolevel1-VirtualNetwork"
            },
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.0.0.0/16"
                    ]
                },
                "subnets": [
                    {
                        "name": "acpdemolevel1-VirtualNetwork-Subnet",
                        "properties": {
                            "addressPrefix": "10.0.0.0/24",
                            "networkSecurityGroup": {
                                "id": "[resourceId('Microsoft.Network/networkSecurityGroups', 'acpdemolevel1-nsg')]"
                            }
                        }
                    }
                ]
            }
        },
        {
            "name": "acpdemolevel1-NetworkInterface",
            "type": "Microsoft.Network/networkInterfaces",
            "apiVersion": "2019-11-01",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/publicIPAddresses', 'acpdemolevel1-PublicIP')]",
                "[resourceId('Microsoft.Network/virtualNetworks', 'acpdemolevel1-VirtualNetwork')]"
            ],
            "tags": {
            },
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "ipConfig1",
                        "properties": {
                            "privateIPAllocationMethod": "Dynamic",
                            "publicIPAddress": {
                                "id": "[resourceId('Microsoft.Network/publicIPAddresses', 'acpdemolevel1-PublicIP')]"
                            },
                            "subnet": {
                                "id": "[resourceId('Microsoft.Network/virtualNetworks/subnets', 'acpdemolevel1-VirtualNetwork', 'acpdemolevel1-VirtualNetwork-Subnet')]"
                            }
                        }
                    }
                ]
            }
        },
        {
            "name": "acpdemolevel1",
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', 'straccacpdemolevel1')]",
                "[resourceId('Microsoft.Network/networkInterfaces', 'acpdemolevel1-NetworkInterface')]"
            ],
            "tags": {

            },
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_A2_v2"
                },
                "osProfile": {
                    "computerName": "acpdemolevel1",
                    "adminUsername": "adminUsername",
                    "adminPassword": "ACPDemoWS01"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2012-R2-Datacenter",
                        "version": "latest"
                    },
                    "osDisk": {
                        "name": "windowsVM1OSDisk",
                        "caching": "ReadWrite",
                        "createOption": "FromImage"
                    }
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[resourceId('Microsoft.Network/networkInterfaces', 'acpdemolevel1-NetworkInterface')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "[reference(resourceId('Microsoft.Storage/storageAccounts/', 'straccacpdemolevel1')).primaryEndpoints.blob]"
                    }
                }
            }
        },
        {
            "name": "acpdemolevel1vault",
            "type": "Microsoft.RecoveryServices/vaults",
            "apiVersion": "2019-05-13",
            "location": "[resourceGroup().location]",
            "dependsOn": [
                "[resourceId('Microsoft.Compute/virtualMachines', 'acpdemolevel1')]"
            ],
            "sku": {
                "name": "RS0",
                "tier": "Standard"
            },
            "properties": {                
            }
        },      
        {
            "type": "Microsoft.RecoveryServices/vaults/backupFabrics/protectionContainers/protectedItems",
            "apiVersion": "2020-02-02",
            "name": "[concat('acpdemolevel1vault/', variables('backupFabric'), '/', variables('protectionContainer'), '/', variables('protectedItem'))]",
            "dependsOn": [
                "[resourceId('Microsoft.Compute/virtualMachines', 'acpdemolevel1')]",
                "[resourceId('Microsoft.RecoveryServices/vaults', 'acpdemolevel1vault')]"
            ],
            "properties": {
                "protectedItemType": "Microsoft.Compute/virtualMachines",
                "policyId": "[resourceId('Microsoft.RecoveryServices/vaults/backupPolicies', 'acpdemolevel1vault', 'DefaultPolicy')]",
                "sourceResourceId": "[resourceId('Microsoft.Compute/virtualMachines', 'acpdemolevel1')]"
            }
        },
        {
            "name": "logAdemolevel1",
            "type": "Microsoft.OperationalInsights/workspaces",
            "apiVersion": "2015-11-01-preview",
            "location": "[resourceGroup().location]",
            "properties": {
                "sku": {
                    "name": "pergb2018"
                },
                "retentionInDays": 30,
                "workspaceCapping": {
                    "dailyQuotaGb": -1
                },
                "features": {
                    "searchVersion": 1
                }
            }
        }
    ],
    "outputs": {}
}